{
  "name": "front-end-test-2",
  "version": "1.0.0",
  "description": "Simple Parcel Sandbox",
  "scripts": {
    "start": "parcel serve ./src/*.html",
    "build": "rimraf dist && parcel build ./src/*.html ./src/index.scss",
    "deploy": "yarn build --global index && surge dist"
  },
  "dependencies": {
    "parcel": "1.12.4",
    "surge": "^0.23.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.6.1",
    "sass": "^1.43.3"
  },
  "keywords": []
}
